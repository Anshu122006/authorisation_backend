generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model User {
  id        Int    @id @default(autoincrement())
  username  String @unique
  email     String @unique
  password  String
  phone    String @unique
  firstname String
  lastname  String
}

model RefreshToken {
  id    Int    @id @default(autoincrement())
  token String @unique
}
